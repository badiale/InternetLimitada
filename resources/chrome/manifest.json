{
  "manifest_version": 2,
  "name": "Internet Limitada",
  "description": "Não espere até 2017 para ter sua internet limitada!",
  "version": "1.0",
  "browser_action": {
    "default_icon": "wifi19.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "scripts": [
      "index.js"
    ]
  },
  "icons": {
    "16": "nowifi16.png",
    "48": "nowifi48.png",
    "128": "nowifi128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "<all_urls>",
    "webRequestBlocking"
  ]
}